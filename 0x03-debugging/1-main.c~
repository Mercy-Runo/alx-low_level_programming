#include <stdio.h>
/**
* main - causes an infinite loop and \\o/ represents an emoji
* Return: 0
* \\o/\n":  emoji
*/

int main(void)
{
int i;

printf("Infinite loop incoming :(\n");

i = 0;
/**
 *while (i < 10) - while loop for i < 10
 *{
 *putchar(i);
 *}
 */
printf("Infinite loop avoided! \\o/\n");

return (0);
}
